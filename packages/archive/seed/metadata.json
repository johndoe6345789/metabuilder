    "package_validator"
  ],
  "exports": {
    "components": [
      "AuditLogViewer",
      "AuditStatsCard",
      "LogTable",
      "LogFilters"
    ],
    "scripts": [
      "init",
      "stats",
      "filters",
      "formatting"
    ]
  },
  "tests": {
    "scripts": [
      "tests/metadata.test.lua",
      "tests/components.test.lua",
      "tests/stats.test.lua",
      "tests/filters.test.lua"
    ],
    "cases": []
