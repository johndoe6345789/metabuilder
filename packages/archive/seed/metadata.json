{
  "packageId": "audit_log",
  "name": "Audit Log",
  "version": "1.0.0",
  "description": "Security audit log viewer and stats dashboard",
  "icon": "static_content/icon.svg",
  "author": "MetaBuilder",
  "category": "admin",
  "dependencies": [],
  "devDependencies": [
