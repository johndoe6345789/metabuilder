{
  "$schema": "https://metabuilder.dev/schemas/json-script.schema.json",
  "schemaVersion": "2.2.0",
  "package": "ui_intro",
  "description": "Intro section initialization and rendering functions",
  "functions": [
    {
      "id": "intro_init",
      "name": "onInstall",
      "exported": true,
      "description": "Package lifecycle handler for installation",
      "category": "lifecycle",
      "luaScript": "init.lua"
    },
    {
      "id": "intro_render",
      "name": "render",
      "exported": true,
      "description": "Renders intro section component",
      "category": "ui",
      "luaScript": "render.lua"
    },
    {
      "id": "intro_validate_props",
      "name": "validateProps",
      "exported": true,
      "description": "Validates intro section props",
      "category": "validation",
      "luaScript": "render.lua"
    }
  ],
  "exports": {
    "functions": [
      "onInstall",
      "render",
      "validateProps"
    ]
  }
}
