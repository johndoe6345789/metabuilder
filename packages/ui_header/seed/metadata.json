{
  "packageId": "ui_header",
  "name": "App Header",
  "version": "1.0.0",
  "description": "Shared navigation header with user avatar and actions",
  "icon": "static_content/icon.svg",
  "author": "MetaBuilder",
  "category": "ui",
  "dependencies": [
    "ui_permissions"
  ],
  "devDependencies": [
    "lua_test"
  ],
  "exports": {
    "components": [
      "AppHeader"
    ],
    "scripts": [
      "header"
    ],
    "pages": []
  },
  "tests": {
    "scripts": [
      "tests/metadata.test.lua",
      "tests/components.test.lua"
    ],
    "cases": [
      "tests/metadata.cases.json",
      "tests/components.cases.json"
    ]
  },
  "minLevel": 2,
  "primary": false,
  "permissions": {
    "header.user.menu": {
      "minLevel": 2,
      "description": "Access user menu"
    },
    "header.view": {
      "minLevel": 2,
      "description": "View application header"
    }
  }
}
