{
  "packageId": "ui_home",
  "name": "Home Page",
  "version": "1.0.0",
  "description": "Level 1 home page with hero, features, about, and contact sections - the original MetaBuilder landing experience",
  "icon": "static_content/icon.svg",
  "author": "MetaBuilder",
  "category": "ui",
  "dependencies": [
    "ui_permissions",
    "ui_header",
    "ui_footer"
  ],
  "devDependencies": [
    "lua_test"
  ],
  "exports": {
    "pages": [
      "level1"
    ],
    "scripts": [
      "home_ui",
      "navigate"
    ],
    "components": [
      "home_page",
      "hero_section",
      "features_section",
      "feature_card_1",
      "feature_card_2",
      "feature_card_3",
      "feature_card_4",
      "feature_card_5",
      "feature_card_6",
      "about_section",
      "contact_section"
    ]
  },
  "tests": {
    "scripts": [
      "tests/metadata.test.lua",
      "tests/components.test.lua"
    ],
    "cases": [
      "tests/metadata.cases.json",
      "tests/components.cases.json"
    ]
  },
  "minLevel": 1,
  "primary": true,
  "permissions": {
    "home.view": {
      "minLevel": 1,
      "description": "View home page"
    },
    "home.server.status": {
      "minLevel": 1,
      "description": "View server status"
    }
  }
}
