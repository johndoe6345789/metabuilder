      "sx": {
        "py": 1,
        "px": 2
      }
    },
    "children": [
      {
        "id": "breadcrumbs_list",
        "type": "Breadcrumbs",
        "props": {
          "separator": "/",
          "className": "breadcrumbs-list",
          "sx": {
            "fontSize": "0.875rem"
          }
        },
        "children": []
      }
    ]
  },
  {
    "id": "breadcrumb_item",
    "type": "Link",
    "props": {
      "underline": "hover",
      "color": "inherit",
      "className": "breadcrumb-item",
      "sx": {
        "display": "flex",
        "alignItems": "center",
        "gap": 0.5,
        "cursor": "pointer"
      }
    },
    "children": [
      {
        "id": "breadcrumb_item_icon",
        "type": "Icon",
        "props": {
          "className": "breadcrumb-item-icon",
          "sx": {
            "fontSize": 18
          }
        },
        "children": []
      },
      {
        "id": "breadcrumb_item_label",
        "type": "Text",
        "props": {
          "variant": "body2",
          "className": "breadcrumb-item-label"
