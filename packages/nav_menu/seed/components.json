  {
    "id": "breadcrumb_separator",
    "type": "Icon",
    "props": {
      "name": "ChevronRight",
      "className": "breadcrumb-separator",
      "sx": {
        "fontSize": 16,
        "color": "text.disabled"
      }
    },
    "children": []
  }
]