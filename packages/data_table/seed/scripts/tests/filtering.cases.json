{
  "matchesFilter": {
    "equals": {
      "matches": [
        { "value": "hello", "filterValue": "hello" },
        { "value": 42, "filterValue": 42 },
        { "value": true, "filterValue": true },
        { "value": "", "filterValue": "" }
      ],
      "no_match": [
        { "value": "hello", "filterValue": "world" },
        { "value": 42, "filterValue": 43 },
        { "value": "Hello", "filterValue": "hello" },
        { "value": true, "filterValue": false }
      ]
    },
    "contains": {
      "matches": [
        { "value": "hello world", "filterValue": "world" },
        { "value": "HELLO WORLD", "filterValue": "hello" },
        { "value": "test@example.com", "filterValue": "@" },
        { "value": "foo bar baz", "filterValue": "bar" }
      ],
      "no_match": [
        { "value": "hello", "filterValue": "world" },
        { "value": "foo", "filterValue": "bar" },
        { "value": "test", "filterValue": "xyz" }
      ]
    },
    "startsWith": {
      "matches": [
        { "value": "hello world", "filterValue": "hello" },
        { "value": "HELLO", "filterValue": "hel" },
        { "value": "test@example.com", "filterValue": "test" },
        { "value": "abc", "filterValue": "a" }
      ],
      "no_match": [
        { "value": "hello world", "filterValue": "world" },
        { "value": "test", "filterValue": "est" },
        { "value": "abc", "filterValue": "bc" }
      ]
    },
    "endsWith": {
      "matches": [
        { "value": "hello world", "filterValue": "world" },
        { "value": "HELLO", "filterValue": "lo" },
        { "value": "test@example.com", "filterValue": ".com" },
        { "value": "abc", "filterValue": "c" }
      ],
      "no_match": [
        { "value": "hello world", "filterValue": "hello" },
        { "value": "test", "filterValue": "tes" },
        { "value": "abc", "filterValue": "ab" }
      ]
    },
    "numeric": {
      "gt": [
        { "value": 10, "filterValue": 5, "expected": true },
        { "value": 5, "filterValue": 10, "expected": false },
        { "value": 5, "filterValue": 5, "expected": false },
        { "value": 0, "filterValue": -1, "expected": true }
      ],
      "lt": [
        { "value": 5, "filterValue": 10, "expected": true },
        { "value": 10, "filterValue": 5, "expected": false },
        { "value": 5, "filterValue": 5, "expected": false },
        { "value": -1, "filterValue": 0, "expected": true }
      ],
      "gte": [
        { "value": 10, "filterValue": 5, "expected": true },
        { "value": 5, "filterValue": 5, "expected": true },
        { "value": 4, "filterValue": 5, "expected": false }
      ],
      "lte": [
        { "value": 5, "filterValue": 10, "expected": true },
        { "value": 5, "filterValue": 5, "expected": true },
        { "value": 6, "filterValue": 5, "expected": false }
      ],
      "between": [
        { "value": 5, "range": [1, 10], "expected": true },
        { "value": 1, "range": [1, 10], "expected": true },
        { "value": 10, "range": [1, 10], "expected": true },
        { "value": 0, "range": [1, 10], "expected": false },
        { "value": 11, "range": [1, 10], "expected": false }
      ]
    },
    "nil_values": [
      { "value": null, "operator": "equals", "filterValue": null, "expected": true, "description": "nil equals nil" },
      { "value": null, "operator": "equals", "filterValue": "test", "expected": false, "description": "nil does not equal string" },
      { "value": null, "operator": "contains", "filterValue": "test", "expected": false, "description": "nil does not contain string" }
    ]
  },
  "applyFilters": {
    "scenarios": [
      {
        "description": "single equals filter",
        "data": [
          { "id": 1, "name": "Alice", "age": 25 },
          { "id": 2, "name": "Bob", "age": 30 },
          { "id": 3, "name": "Alice", "age": 35 }
        ],
        "filters": [{ "column": "name", "operator": "equals", "value": "Alice" }],
        "expectedCount": 2,
        "expectedIds": [1, 3]
      },
      {
        "description": "single numeric gt filter",
        "data": [
          { "id": 1, "value": 10 },
          { "id": 2, "value": 20 },
          { "id": 3, "value": 30 }
        ],
        "filters": [{ "column": "value", "operator": "gt", "value": 15 }],
        "expectedCount": 2,
        "expectedIds": [2, 3]
      },
      {
        "description": "multiple filters (AND logic)",
        "data": [
          { "id": 1, "name": "Alice", "active": true },
          { "id": 2, "name": "Bob", "active": true },
          { "id": 3, "name": "Alice", "active": false }
        ],
        "filters": [
          { "column": "name", "operator": "equals", "value": "Alice" },
          { "column": "active", "operator": "equals", "value": true }
        ],
        "expectedCount": 1,
        "expectedIds": [1]
      },
      {
        "description": "no matching rows",
        "data": [
          { "id": 1, "value": 10 },
          { "id": 2, "value": 20 }
        ],
        "filters": [{ "column": "value", "operator": "gt", "value": 100 }],
        "expectedCount": 0
      }
    ]
  }
}
