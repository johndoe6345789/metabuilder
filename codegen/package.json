    "@metabuilder/hooks-canvas": "workspace:*",
    "@metabuilder/hooks-core": "workspace:*",
    "@metabuilder/hooks-data": "workspace:*",
    "@metabuilder/redux-slices": "workspace:*",
    "@metabuilder/service-adapters": "workspace:*",
    "@metabuilder/timing-utils": "workspace:*",
    "@monaco-editor/react": "^0.63.2",
    "@radix-ui/react-accordion": "^1.1.2",
    "@radix-ui/react-alert-dialog": "^1.1.6",
    "@radix-ui/react-aspect-ratio": "^1.1.2",
    "@radix-ui/react-avatar": "^1.1.2",
    "@radix-ui/react-checkbox": "^1.1.2",
    "@radix-ui/react-collapsible": "^1.1.2",
    "@radix-ui/react-context-menu": "^2.2.6",
    "@radix-ui/react-dialog": "^1.1.8",
    "@radix-ui/react-dropdown-menu": "^2.1.6",
    "@radix-ui/react-hover-card": "^1.1.6",
    "@radix-ui/react-label": "^2.1.2",
