{
  "name": "@metabuilder/redux-core",
  "version": "1.0.0",
  "description": "Core Redux slices for MetaBuilder - shared across all frontends",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@reduxjs/toolkit": "^2.0.0",
    "react": "^18.3.1",
    "react-redux": "^9.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.45",
    "typescript": "^5.3.3"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    },
    "./slices": {
      "types": "./dist/slices/index.d.ts",
      "default": "./dist/slices/index.js"
    },
    "./types": {
      "types": "./dist/types/index.d.ts",
      "default": "./dist/types/index.js"
    },
    "./store": {
      "types": "./dist/store/index.d.ts",
      "default": "./dist/store/index.js"
    }
  }
}
