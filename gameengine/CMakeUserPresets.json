{
    "version": 4,
    "vendor": {
        "conan": {}
    },
    "configurePresets": [
        {
            "name": "vita-release",
            "inherits": [
                "conan-release"
            ],
            "cacheVariables": {
                "ENABLE_VITA": "ON"
            },
            "environment": {
                "VITASDK": "/usr/local/vitasdk"
            }
        }
    ],
    "include": [
        "build-ninja/build/Release/generators/CMakePresets.json"
    ]
}